   "source": [
    "from mxnet import autograd, nd\n",
    "x = nd.arange(1,5)\n",
    "print(x)\n",
    "y = 3\n",
    "x.attach_grad()\n",
    "with autograd.record():\n",
    "    z = y**x\n",
    "z.backward()\n",
    "x.grad"
   ]
